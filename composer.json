{
  "name": "overplex/yii2wajaxcrud",
  "description": "Gii CRUD template for Single Page Ajax Administration for yii2",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "ajax",
    "crud",
    "gii",
    "template",
    "database"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "overplex",
      "email": "vl.haritonov@gmail.com",
      "homepage": "https://github.com/overplex?tab=repositories"
    }
  ],
  "support": {
    "issues": "https://github.com/overplex/yii2wajaxcrud/issues?state=open",
    "source": "https://github.com/overplex/yii2wajaxcrud"
  },
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "^2.0.6",
    "yiisoft/yii2-gii": "^2.1.0",
    "yiisoft/yii2-bootstrap4": "^2.0.8",
    "bower-asset/bootstrap": "^4.0.0",
    "kartik-v/yii2-krajee-base": "^2.0.5",
    "kartik-v/yii2-grid": "^3.3.4",
    "kartik-v/yii2-detail-view": "^1.8.2",
    "kartik-v/yii2-date-range": "^1.7.1",
    "kartik-v/yii2-mpdf": "^1.0.5",
    "kartik-v/yii2-editable": "^1.7.8",
    "kartik-v/yii2-widget-activeform": "^1.5.8",
    "wodrow/yii2wtools": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "overplex\\wajaxcrud\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "overplex\\wajaxcrud\\Bootstrap"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
